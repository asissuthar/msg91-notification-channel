{
    "name": "asissuthar/msg91-notification-channel",
    "description": "Nexmo Notification Channel for laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ashish Suthar",
            "email": "asissuthar@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "asissuthar/msg91-laravel": "dev-master",
        "illuminate/notifications": "^6.16"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Notifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminate\\Tests\\Notifications\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Illuminate\\Notifications\\Msg91ChannelServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
